# Graphical desktop {{{
alias which-gd="cat /etc/X11/default-display-manager"
update-gd(){
if [ "$*" ]; then
	sudo dpkg-reconfigure "$*"
else
	echo 'sudo dpkg-reconfigure <gdm,kdm,...>'
fi
}
