# VirtualBox {{{
vm(){
	if [ "$*" ]; then
		vboxmanage startvm "$*" --type headless
	else
		vboxmanage list vms
	fi
}
killvm(){
	if [ "$*" ]; then
		vboxmanage controlvm "$*" poweroff soft
	else
		vboxmanage list runningvms
	fi
}
